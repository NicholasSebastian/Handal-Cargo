{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "devPath": "http://localhost:3000",
    "distDir": "../build",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Handal Cargo Management",
    "version": "8.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "scope": [
          {
            "name": "wmic",
            "cmd": "wmic",
            "args": true
          },
          {
            "name": "tasklist",
            "cmd": "tasklist",
            "args": true
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "Business",
      "copyright": "Handal Cargo",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.nicholas.handalcargo",
      "longDescription": "Handal Cargo Integrated Cargo Management System",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "webviewInstallMode": {
          "type": "offlineInstaller",
          "silent": false
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "title": "Handal Cargo Integrated System",
        "resizable": false,
        "height": 400,
        "width": 600
      }
    ]
  }
}